#include <stdio.h>

int findMissingBinary(int arr[], int low, int high) {
    if (low > high)
        return high + 2; // missing number

    int mid = (low + high) / 2;

    // Check if missing number is at mid
    if (arr[mid] != mid + 1)
    {
        // If previous is correct, then missing is at mid
        if (mid == 0 || arr[mid - 1] == mid)
            return mid + 1;
        else
            return findMissingBinary(arr, low, mid - 1);
    }
    else
        return findMissingBinary(arr, mid + 1, high);
}

int main() {
    int n;
    printf("Enter the value of n: ");
    scanf("%d", &n);

    int arr[n-1];
    printf("Enter %d sorted distinct elements (from 1 to %d) with one missing:\n", n-1, n);
    for(int i = 0; i < n-1; i++) {
        scanf("%d", &arr[i]);
    }

    int missing = findMissingBinary(arr, 0, n-2);
    printf("Missing number is %d\n", missing);

    return 0;
}
